@{
	ViewData["Title"] = "Home Page";
}
@model List<WarehouseModel>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<div class="text-center" style="margin-bottom:35px;">
    <h1>Склады</h1>
</div>

<table class="table table-striped">
	<tbody>
		<tr>
			<td><h5>№</h5></td>
			<td><h5>Имя склада</h5></td>
			<td><h5>Адрес</h5></td>
			<td><h5>Вместимость</h5></td>
			<td><h5>Дата открытия</h5></td>
			<td><h5>Эксплуатируется ли</h5></td>
		</tr>
		@for (var i = 0; i < Model.Count; i++)
		{
			

			<tr>
				<td>@(i+1)</td>
				<td><a class="link-offset-2 link-underline link-underline-opacity-0" href='@Url.Action( "WarehouseProducts", "Home", Model[i])'>@Model[i].Name</a></td>
				<td>@Model[i].Address</td>
				<td>@Model[i].Capacity</td>
				<td>
					@if (Model[i].OpenDateTime != null)
					{
						@Model[i].OpenDateTime.Value.ToString("d MMMM yyyyг. hh:mm:ss");
					}
				</td>
				<td>@Model[i].IsWorking</td>
			</tr>
		}
	</tbody>
</table>